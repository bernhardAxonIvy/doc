msgstr ""

#: ../../source/engine-guide/reference/engine-cockpit/index.rst:51
msgid "License messages"
msgstr ""

#: ../../source/engine-guide/reference/engine-cockpit/index.rst:52
msgid "Cluster warning"
msgstr ""

#: ../../source/engine-guide/reference/engine-cockpit/index.rst:53
msgid "Health messages"
msgstr ""

#: ../../source/engine-guide/reference/engine-cockpit/index.rst:54
msgid "Documentation"
msgstr ""

#: ../../source/engine-guide/reference/engine-cockpit/index.rst:55
msgid "Switch between dark and light mode"
msgstr ""

#: ../../source/engine-guide/reference/engine-cockpit/index.rst:56
msgid "Administrator user menu"
msgstr ""

#: ../../source/engine-guide/reference/engine-cockpit/index.rst:58
msgid "Some items may not be available or shown depending on your engine."
